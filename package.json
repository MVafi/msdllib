  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.3",
    "@types/node": "^22.13.1",
    "@types/utm": "^1.1.4",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  }
